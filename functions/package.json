    "logs": "firebase functions:log",
    "kill": "npx kill-port 5000 5001 8080 8085 4000 9229 21526"
  },
  "engines": {
    "node": "16"
  },
  "main": "index.js",
  "dependencies": {
    "firebase-admin": "^10.0.2",
    "firebase-functions": "^3.18.0",
    "firebase-tools": "^9.23.3",
    "node-fetch": "^2.6.1",
    "nodemailer": "^6.7.7",
    "openai": "^3.2.1",
    "retry": "^0.13.1"
  },
  "devDependencies": {
    "eslint": "^8.9.0",
    "eslint-config-google": "^0.14.0",
    "firebase-functions-test": "^0.2.0"
  },
  "private": true
}
